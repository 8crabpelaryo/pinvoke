<Project>
  <ItemGroup>
    <Compile Remove="desktop\**" Condition=" '$(TargetFramework)' != 'net40' " />
  </ItemGroup>

  <Target Name="BuildProjectReferencesFirst" BeforeTargets="ResolveProjectReferences">
    <MSBuild Projects="@(BuildProjectReference)"
             BuildInParallel="$(BuildInParallel)"
             Properties="Configuration=$(Configuration);Platform=$(Platform);TargetFramework=net46" />
  </Target>
</Project>
